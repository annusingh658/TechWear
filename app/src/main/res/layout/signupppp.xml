<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    android:padding="16dp">


    <ImageView
        android:id="@+id/back_button"
        android:layout_width="22dp"
        android:layout_height="28dp"
        android:background="?attr/selectableItemBackground"
        android:contentDescription="Back"
        android:src="@drawable/navarroww"
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="26dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="113dp"
        android:layout_height="45dp"
        android:text="Sign up"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="76dp" />

    <EditText
        android:id="@+id/username"
        android:layout_width="375dp"
        android:layout_height="52dp"
        android:background="@drawable/roundedcorners"
        android:hint="Username"
        android:padding="12dp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="179dp" />

    <EditText
        android:id="@+id/email"
        android:layout_width="375dp"
        android:layout_height="52dp"
        android:background="@drawable/roundedcorners"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:padding="12dp"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="252dp" />

    <EditText
        android:id="@+id/password"
        android:layout_width="375dp"
        android:layout_height="52dp"
        android:background="@drawable/roundedcorners"
        android:hint="Password"
        android:inputType="textPassword"
        android:padding="12dp"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="327dp" />

    <EditText
        android:id="@+id/confirm_password"
        android:layout_width="375dp"
        android:layout_height="52dp"
        android:background="@drawable/roundedcorners"
        android:hint="Confirm password"
        android:inputType="textPassword"
        android:padding="12dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="395dp" />

    <Button
        android:id="@+id/register_button"
        android:layout_width="375dp"
        android:layout_height="52dp"
        android:backgroundTint="#12C6CB"
        android:padding="12dp"
        android:text="Agree and Register"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="483dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Or Login with"
        android:textColor="@color/black"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="553dp" />

    <ImageView
        android:id="@+id/facebook_login"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:contentDescription="Login with Facebook"
        android:padding="8dp"
        android:src="@drawable/facebook457"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="588dp" />

    <ImageView
        android:id="@+id/google_login"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:contentDescription="Login with Google"
        android:padding="8dp"
        android:src="@drawable/google567"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="588dp" />

    <ImageView
        android:id="@+id/apple_login"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:contentDescription="Login with Apple"
        android:padding="8dp"
        android:src="@drawable/facebook457"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="588dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
